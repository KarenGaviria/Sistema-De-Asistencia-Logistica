@model SistemaDeAsistenciaLogistica.Core.Domain.Produccion

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAdmi.cshtml";
}

<h1>Detalles</h1>

<div>
    <h4>Produccion</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Fecha Produccion 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaProduccion)
        </dd>
        <dt class = "col-sm-2">
           Detalle Produccion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DetalleProducto)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cantidad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cantidad)
        </dd>
        <dt class = "col-sm-2">
           Estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEstadoNavigation.EstadoProduccion1)
        </dd>
       
        <dt class = "col-sm-2">
           Producto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdProductoNavigation.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdProduccion">Editar</a> |
    <a asp-action="Index">Atrás</a>
</div>
