@model SistemaDeAsistenciaLogistica.Core.Domain.TipoProducto

@{ ViewData["Title"] = "Ingresar";
                Layout = "~/Views/Shared/_LayoutAdmi.cshtml"; }

@section ScriptValidacion{
    <script src="~/lib/bootstrap-validator/bootstrapValidator.js"></script>
    <script src="~/js/validacionFormNombreProducto.js"></script>
}


<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" id="createNombreProductoForm">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NombreProducto" class="control-label">Tipo de Producto</label>
                <input asp-for="NombreProducto" class="form-control" />
                <span asp-validation-for="NombreProducto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Ingresar" class="btn btn-primary" />
                <a asp-action="Index"><i class="icon ion-ios-undo"></i></a>
            </div>
        </form>
    </div>
</div>



