@model SistemaDeAsistenciaLogistica.Core.Domain.Inventario

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAdmi.cshtml";
}

<h1>Detalles</h1>

<div>
    <h4>Inventario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Fecha Ingreso
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaIngreso)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cantidad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cantidad)
        </dd>
        <dt class = "col-sm-2">
          Valor Unidad/
          Valor Metro
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ValorUnidadValorMetro)
        </dd>
        <dt class = "col-sm-2">
           Valor Total
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ValorTotal)
        </dd>
        <dt class = "col-sm-2">
           Insumo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdInsumoNavigation.NombreInsumo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdPerfilNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdPerfilNavigation.PrimerApellido)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdInventario">Editar</a> |
    <a asp-action="Index">Atrás</a>
</div>
